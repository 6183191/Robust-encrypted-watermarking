B = zeros(960,1280)
max_intesity = 255 
for m = 2:960 
   for n = 1
      x = B(m-1,1) 
      x = x*2 
      y = max_intesity - x 
      B(m,n) = abs(y) 
   end 
end
for i = 1:960 
   for j = 1:1280 
       p = B(i,j-1)
       p = p*2 
       q = max_intesity - p  
       B(m,n) = abs(q)
   end
end